{
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.sh"
    }
  ],
  "headers": [
    {
      "source": "/(.*).sh",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/plain"
        },
        {
          "key": "Content-Disposition",
          "value": "inline"
        }
      ]
    }
  ]
}